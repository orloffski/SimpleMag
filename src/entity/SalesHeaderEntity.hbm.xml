<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.SalesHeaderEntity" table="sales_header" schema="minimag">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="salesNumber">
            <column name="sales_number" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="summ">
            <column name="summ" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="salesType">
            <column name="sales_type" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="payment">
            <column name="payment" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="lastcreateupdate">
            <column name="lastcreateupdate" sql-type="timestamp"/>
        </property>
    </class>
</hibernate-mapping>