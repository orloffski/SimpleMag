<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.CounterpartiesEntity" table="counterparties" schema="minimag">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="adress">
            <column name="adress" sql-type="varchar(150)" length="150" not-null="true"/>
        </property>
        <property name="unn">
            <column name="UNN" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>