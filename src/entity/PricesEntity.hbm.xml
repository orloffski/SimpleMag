<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.PricesEntity" table="prices" schema="minimag">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="price">
            <column name="price" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="lastcreated">
            <column name="lastcreated" sql-type="timestamp"/>
        </property>
        <property name="reason">
            <column name="reason" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>