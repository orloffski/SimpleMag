<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.InvoicesLinesEntity" table="invoices_lines" schema="minimag">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="lineNumber">
            <column name="line_number" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="invoiceNumber">
            <column name="invoice_number" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="itemId">
            <column name="item_id" sql-type="int(11)"/>
        </property>
        <property name="vendorPrice">
            <column name="vendor_price" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="vat">
            <column name="vat" sql-type="tinyint(2)" not-null="true"/>
        </property>
        <property name="extraPrice">
            <column name="extra_price" sql-type="tinyint(2)" not-null="true"/>
        </property>
        <property name="retailPrice">
            <column name="retail_price" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="itemName">
            <column name="item_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="summVat">
            <column name="summ_vat" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="summInclVat">
            <column name="summ_incl_vat" sql-type="double" precision="-1" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>