<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.InvoicesHeadersEntity" table="invoices_headers" schema="minimag">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="number">
            <column name="number" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="type">
            <column name="type" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="counterparty">
            <column name="counterparty" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="count">
            <column name="count" sql-type="int(3)" not-null="true"/>
        </property>
        <property name="summ">
            <column name="summ" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="counterpartyId">
            <column name="counterparty_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="lastcreated">
            <column name="lastcreated" sql-type="timestamp"/>
        </property>
        <property name="recipientId">
            <column name="recipient_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="recipientName">
            <column name="recipient_name" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="fullSumm">
            <column name="full_summ" sql-type="double" precision="-1" not-null="true"/>
        </property>
        <property name="ttnNumber">
            <column name="ttn_number" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="ttnDate">
            <column name="ttn_date" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>