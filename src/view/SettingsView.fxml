<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SettingsViewController">
   <center>
      <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0">
         <children>
            <HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0" prefWidth="800.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="570.0" prefWidth="250.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" top="15.0" />
                     </HBox.margin>
                     <children>
                        <Label text="Настройки справочников">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                        <Text fx:id="counterpartiesSettings" onMouseClicked="#openCounterpartiesSettings" onMouseEntered="#setCounterpartiesTextUnderline" onMouseExited="#removeCounterpartiesTextUnderline" strokeType="OUTSIDE" strokeWidth="0.0" text="Контрагенты">
                           <VBox.margin>
                              <Insets left="25.0" />
                           </VBox.margin>
                        </Text>
                        <Text fx:id="unitsSettings" onMouseClicked="#openUnitsSettings" onMouseEntered="#setUnitsTextUnderline" onMouseExited="#removeUnitsTextUnderline" strokeType="OUTSIDE" strokeWidth="0.0" text="Единицы измерения">
                           <VBox.margin>
                              <Insets left="25.0" />
                           </VBox.margin>
                        </Text>
                        <Text fx:id="itemsList" onMouseClicked="#openItemsView" onMouseEntered="#setItemsTextUnderline" onMouseExited="#removeItemsTextUnderline" strokeType="OUTSIDE" strokeWidth="0.0" text="Список товаров">
                           <VBox.margin>
                              <Insets left="25.0" />
                           </VBox.margin>
                        </Text>
                     </children>
                  </VBox>
                  <VBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="570.0" prefWidth="250.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="15.0" top="15.0" />
                     </HBox.margin>
                     <children>
                        <Label text="Настройки программы">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Label>
                        <Text fx:id="productsInStock" onMouseClicked="#openProductsInStockSettings" onMouseEntered="#setProductsInStockUnderline" onMouseExited="#removeProductsInStockUnderline" strokeType="OUTSIDE" strokeWidth="0.0" text="Контроль остатков склада">
                           <VBox.margin>
                              <Insets left="25.0" />
                           </VBox.margin>
                        </Text>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children></AnchorPane>
   </center>
</BorderPane>
